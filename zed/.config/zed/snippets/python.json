{
  "Main entry point": {
    "prefix": "ifmain",
    "description": "Main entry point template",
    "body": ["if __name__ == '__main__':", "    main()", ""],
  },
  "Create a function": {
    "prefix": "def",
    "description": "Function template",
    "body": [
      "def ${1:new_function}($2) -> ${3:None}:",
      "    ${4:raise NotImplementedError('$1() was not implemented.')}",
    ],
  },
  "Quick debug": {
    "prefix": "debug",
    "description": "Quick debug",
    "body": ["print(f'${1:name} = {${2:var}}')", "$0"],
  },
  "Quick try/except": {
    "prefix": "try",
    "description": "Quick try/except block",
    "body": [
      "try:",
      "    ${1:...}",
      "except Exception as e:",
      "    print(f'{type(e).__name__}: {e}')",
      "$0",
    ],
  },
}
